{
  "cli": {
    "version": ">= 14.0.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_OPENAI_API_KEY": ""
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_OPENAI_API_KEY": ""
      }
    }
  },
  "submit": {
    "production": {}
  }
}
